@model HtmlInputViewModel<UITextbox>
@{
    UIContainer container = Model.Parameters["Container"] as UIContainer;
    UITextbox textbox = Model.Input;
}
<div class="col-md-12 form-group">
    <label for="@container.Guid">@textbox.DisplayName</label>
    <input class="form-control" type="text" id="@container.Guid" name="@textbox.Name" value="@textbox.Text" placeholder="@textbox.Placeholder" />
</div>


